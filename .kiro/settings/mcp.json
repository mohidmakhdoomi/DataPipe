{
  "mcpServers": {
    "clearthought": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "waldzellai/clear-thought"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "zen": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env-file",
        "E:/Projects/DataPipe/.kiro/settings/zen-mcp-server/.env",
        "-v",
        "E:/Projects/DataPipe/.kiro/settings/zen-mcp-server/logs:/app/logs",
        "zen-mcp-server:latest"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "chat",
        "thinkdeep",
        "challenge",
        "planner",
        "consensus",
        "codereview",
        "precommit",
        "debug",
        "analyze",
        "refactor",
        "tracer",
        "testgen",
        "secaudit",
        "docgen"
      ]
    },
    "zenORIG": {
      "command": "sh",
      "args": [
        "-c",
        "exec uvx --from git+https://github.com/BeehiveInnovations/zen-mcp-server.git zen-mcp-server"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "terraform": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "sequentialthinking": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "sequentialthinking"
      ]
    },
    "Context7": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "context7-mcp"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "get-library-docs",
        "resolve-library-id"
      ]
    },
    "aws-terraform": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-terraform"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": [
        "SearchAwsProviderDocs",
        "SearchAwsccProviderDocs",
        "SearchSpecificAwsIaModules",
        "SearchUserProvidedModule",
        "RunCheckovScan"
      ]
    },
    "aws-cdk-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-cdk-mcp-server"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "aws-diagram": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-diagram"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "aws-documentation": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-documentation"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": [
        "search_documentation",
        "read_documentation"
      ]
    },
    "aws-core-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-core-mcp-server"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": [
        "prompt_understanding"
      ]
    },
    "docker": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/docker"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    }
  }
}