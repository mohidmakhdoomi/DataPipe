{
  "mcpServers": {
    "clear-thought-proxy": {
      "command": "mcp-proxy",
      "args": [
        "--transport",
        "streamablehttp",
        "http://localhost:3000/mcp"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "clear-thought-starter": {
      "command": "sh .kiro/settings/startClearThoughtDocker.sh",
      "args": [],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "clear-thought": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "waldzellai/clear-thought"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "sequentialthinking",
        "mentalmodel",
        "debuggingapproach",
        "collaborativereasoning",
        "decisionframework",
        "metacognitivemonitoring",
        "scientificmethod",
        "structuredargumentation",
        "visualreasoning"
      ]
    },
    "zen": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--env-file",
        "E:/Projects/DataPipe/.kiro/settings/zen/.env",
        "-v",
        "E:/Projects/DataPipe/.kiro/settings/zen/logs:/app/logs",
        "zen-mcp-server"
      ],
      "timeout": 360000,
      "env": {
        "MCP_SERVER_REQUEST_TIMEOUT": "360000",
        "DEFAULT_REQUEST_TIMEOUT_MSEC": "360000",
        "MCP_TIMEOUT": "360000",
        "DEFAULT_TOOL_CALL_TIMEOUT": "360000",
        "CUSTOM_CONNECT_TIMEOUT": "360000"
      },
      "disabled": false,
      "autoApprove": [
        "chat",
        "thinkdeep",
        "challenge",
        "planner",
        "consensus",
        "codereview",
        "precommit",
        "debug",
        "analyze",
        "refactor",
        "tracer",
        "testgen",
        "secaudit",
        "docgen"
      ]
    },
    "Context7": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "context7-mcp"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "get-library-docs",
        "resolve-library-id"
      ]
    },
    "sequentialthinking": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": [
        "sequentialthinking"
      ]
    },
    "terraform": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "aws-terraform": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-terraform"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": [
        "SearchAwsProviderDocs",
        "SearchAwsccProviderDocs",
        "SearchSpecificAwsIaModules",
        "SearchUserProvidedModule",
        "RunCheckovScan"
      ]
    },
    "aws-cdk-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-cdk-mcp-server"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "aws-diagram": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-diagram"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": []
    },
    "aws-documentation": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-documentation"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": [
        "search_documentation",
        "read_documentation"
      ]
    },
    "aws-core-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/aws-core-mcp-server"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": true,
      "autoApprove": [
        "prompt_understanding"
      ]
    },
    "docker": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/docker"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    }
  }
}